        {
          "listen": "test",
          "script": {
            "type": "text/javascript",
            "exec": [
              "// ============================================",
              "// LOAD TEST POST-RESPONSE SCRIPT",
              "// Tracks results and generates summary report",
              "// ============================================",
              "",
              "const currentIteration = pm.info.iteration + 1;",
              "const totalIterations = 150;",
              "const responseTime = pm.response.responseTime;",
              "const statusCode = pm.response.code;",
              "",
              "// Get current counters",
              "let successCount = parseInt(pm.collectionVariables.get('loadTest_successCount')) || 0;",
              "let rateLimitedCount = parseInt(pm.collectionVariables.get('loadTest_rateLimitedCount')) || 0;",
              "let otherErrorCount = parseInt(pm.collectionVariables.get('loadTest_otherErrorCount')) || 0;",
              "let totalResponseTime = parseInt(pm.collectionVariables.get('loadTest_totalResponseTime')) || 0;",
              "let minResponseTime = parseInt(pm.collectionVariables.get('loadTest_minResponseTime')) || 999999;",
              "let maxResponseTime = parseInt(pm.collectionVariables.get('loadTest_maxResponseTime')) || 0;",
              "let responseTimes = JSON.parse(pm.collectionVariables.get('loadTest_responseTimes') || '[]');",
              "let statusCodes = JSON.parse(pm.collectionVariables.get('loadTest_statusCodes') || '{}');",
              "",
              "// Update counters based on response",
              "if (statusCode === 200) {",
              "    successCount++;",
              "    console.log(`‚úÖ Request ${currentIteration}: SUCCESS (200) - ${responseTime}ms`);",
              "} else if (statusCode === 429) {",
              "    rateLimitedCount++;",
              "    console.log(`üö´ Request ${currentIteration}: RATE LIMITED (429) - ${responseTime}ms`);",
              "} else {",
              "    otherErrorCount++;",
              "    console.log(`‚ùå Request ${currentIteration}: ERROR (${statusCode}) - ${responseTime}ms`);",
              "}",
              "",
              "// Track response times",
              "totalResponseTime += responseTime;",
              "minResponseTime = Math.min(minResponseTime, responseTime);",
              "maxResponseTime = Math.max(maxResponseTime, responseTime);",
              "responseTimes.push(responseTime);",
              "",
              "// Track status code distribution",
              "statusCodes[statusCode] = (statusCodes[statusCode] || 0) + 1;",
              "",
              "// Save updated counters",
              "pm.collectionVariables.set('loadTest_successCount', successCount);",
              "pm.collectionVariables.set('loadTest_rateLimitedCount', rateLimitedCount);",
              "pm.collectionVariables.set('loadTest_otherErrorCount', otherErrorCount);",
              "pm.collectionVariables.set('loadTest_totalResponseTime', totalResponseTime);",
              "pm.collectionVariables.set('loadTest_minResponseTime', minResponseTime);",
              "pm.collectionVariables.set('loadTest_maxResponseTime', maxResponseTime);",
              "pm.collectionVariables.set('loadTest_responseTimes', JSON.stringify(responseTimes));",
              "pm.collectionVariables.set('loadTest_statusCodes', JSON.stringify(statusCodes));",
              "",
              "// ============================================",
              "// TESTS",
              "// ============================================",
              "",
              "pm.test('Response status is either 200 (OK) or 429 (Rate Limited)', function() {",
              "    pm.expect(statusCode).to.be.oneOf([200, 429]);",
              "});",
              "",
              "pm.test('Response time is reasonable (< 1000ms)', function() {",
              "    pm.expect(responseTime).to.be.below(1000);",
              "});",
              "",
